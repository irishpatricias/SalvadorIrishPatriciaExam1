<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view6"
        style="@style/Background"
        android:layout_width="wrap_content"
        android:layout_height="683dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991" />

    <TextView
        android:id="@+id/textView3"
        style="@style/TextviewStyle"
        android:layout_width="360dp"
        android:layout_height="125dp"
        android:layout_gravity="center"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="356dp"
        android:text="Marvel"
        android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Marvel" />

    <TextView
        android:id="@+id/textView"
        style="@style/TextviewStyle"
        android:layout_width="360dp"
        android:layout_height="125dp"
        android:layout_gravity="center"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="212dp"
        android:text="DC"
        android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="DC" />

    <TextView
        android:id="@+id/textView4"
        style="@style/TextviewStyle"
        android:layout_width="360dp"
        android:layout_height="131dp"
        android:layout_gravity="center"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="60dp"
        android:text="Pixar"
        android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Pixar" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="248dp"
        android:layout_height="71dp"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="92dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.805"
        app:layout_constraintStart_toEndOf="@+id/view6"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        app:layout_constraintVertical_bias="0.876"
        app:srcCompat="@drawable/marvel" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="158dp"
        android:layout_height="62dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="81dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.076"
        app:srcCompat="@drawable/dc" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="139dp"
        android:layout_height="67dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="9dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.575"
        app:srcCompat="@drawable/maxresdefault" />

    <TextView
        android:id="@+id/textView14"
        style="@style/ContentStyle"
        android:layout_width="342dp"
        android:layout_height="59dp"
        android:layout_gravity="center"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="356dp"
        android:text="Marvel Entertainment, LLC is an American entertainment company founded in June 1998 and based in New York City."
        android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Marvel Entertainment, LLC is an American entertainment company founded in June 1998 and based in New York City." />

    <TextView
        android:id="@+id/textView16"
        style="@style/ContentStyle"
        android:layout_width="341dp"
        android:layout_height="59dp"
        android:layout_gravity="center"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="212dp"
        android:text="DC Comics Inc. is the world's largest comic book company and is perhaps best known for publishing the adventures of Superman and Batman."
        android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="DC Comics Inc. is the world's largest comic book company and is perhaps best known for publishing the adventures of Superman and Batman." />

    <TextView
        android:id="@+id/textView15"
        style="@style/ContentStyle"
        android:layout_width="340dp"
        android:layout_height="59dp"
        android:layout_gravity="center"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="60dp"
        android:text="Pixar Animation Studio, is an American computer animation film studio based in California that is a subsidiary of Walt Disney Studios."
        android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Pixar Animation Studio, is an American computer animation film studio based in California that is a subsidiary of Walt Disney Studios." />

</android.support.constraint.ConstraintLayout>